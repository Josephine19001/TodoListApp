<%- include("header") %> 
    <!-- EJS marker = -->
        <div class="box" id="title">
            <h1><%= listTitle %> </h1>
        </div>
        
        <div class="box">
            
                <% for(let i= 0; i < newListItem.length; i++){%>
                    <div class="item">
                       <input type="checkbox"> 
                    <p><%= newListItem[i] %> </p> 
                    </div>
                    
                <% } %> 
                <form action="/" method="post">
                    <input type="text" name="newItem" placeholder="Add a ToDo" autocomplete="off">
                    <button type="submit" name="list"value=<%=listTitle%> >+</button>
                </form>
        </div>
        
   <%- include("footer") %> 